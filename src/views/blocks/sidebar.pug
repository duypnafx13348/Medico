.dashboard__main__sidebar 
    ul.dashboard__main__sidebar__list
        each value in sidebarList
            li(class="dashboard__main__sidebar__item") 
                div(class="d-flex justify-content-between align-items-start dashboard__main__sidebar__item-event")
                    div(class="d-flex align-items-start") !{value.image}
                        a(href= value.url, class="link")= value.title
                    if (value.listChild.length > 0)
                        i(class="fa fa-chevron-right" aria-hidden="true")
                if(value.listChild.length > 0)
                    ul.dashboard__main__sidebar__list-child
                        each valChild, index in value.listChild
                            li.dashboard__main__sidebar__item-child
                                a(href= valChild.url, class="link__child")= valChild.titleChild 